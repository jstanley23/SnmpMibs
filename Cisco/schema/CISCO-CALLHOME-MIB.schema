	group	ccmCallHomeStatus
	description "(null)"
	characteristics "-K NONE"
	(
		readonly timeticks	ccmSmtpServerStatusTimeStamp
		description 	"This object indicates the time when ccmSmtpServerStatusTable
        was last updated, either by a SNMP Servers check by 
        ccmSmtpServerStatusInitiate or by a change in the SMTP
        servers configured in ccmSmtpServersTable."
		characteristics "-N ccmSmtpServerStatusTimeStamp -O 1.3.6.1.4.1.9.9.300.1.3.1 -T TIMETICKS -A RO -X equal -F 0"

		readwrite enum ccmSmtpServerStatusInitiateE	ccmSmtpServerStatusInitiate
		description 	"A control object used to initiate the availability status
        check of the SMTP Servers configured in ccmSmtpServersTable.
        
        Setting this object to 'true' will check the status of
        the SMTP servers and update the status in 
        ccmSmtpServerStatusTable. This object cannot be set to 'true'
        when the value of ccmSmtpServerStatusResult is 'inprogress'.
        
        Setting this object to 'false' has no effect.
        
        When read, this object always returns 'false'."
		characteristics "-N ccmSmtpServerStatusInitiate -O 1.3.6.1.4.1.9.9.300.1.3.2 -T INTEGER -A RW -X equal -F 0"

		readonly enum ccmSmtpServerStatusResultE	ccmSmtpServerStatusResult
		description 	"This object provides the status of the last SMTP status
        check initiated by ccmSmtpServerStatusInitiate.
        
        Management stations could read value of this object after a
        successful set of the ccmSmtpServerStatusInitiate object to
        find out the result of the SMTP server status check."
		characteristics "-N ccmSmtpServerStatusResult -O 1.3.6.1.4.1.9.9.300.1.3.3 -T INTEGER -A RO -X equal -F 0"

		readonly enum ccmOnDemandMsgServAvailableE	ccmOnDemandMsgServAvailable
		description 	"This object indicates whether the system is ready to generate
        on demand Call Home messages.
        
        A value of 'true' indicates that the system is ready to generate
        on demand Call Home messages. A value of 'false' indicates that
        sending of on demand Call Home messages can not be initiated."
		characteristics "-N ccmOnDemandMsgServAvailable -O 1.3.6.1.4.1.9.9.300.1.3.5 -T INTEGER -A RO -X equal -F 0"

	)

