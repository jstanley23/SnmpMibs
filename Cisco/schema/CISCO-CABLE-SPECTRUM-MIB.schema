		characteristics "-N ccsSNRRequestOperState -O 1.3.6.1.4.1.9.9.114.1.2.3.1.5 -T INTEGER -A RO -X equal -F 0"

		readonly timeticks	ccsSNRRequestStartTime
		description 	"The value of sysUpTime when the CNR measurement
        operation starts."
		characteristics "-N ccsSNRRequestStartTime -O 1.3.6.1.4.1.9.9.114.1.2.3.1.6 -T TIMETICKS -A RO -X equal -F 0"

		readonly timeticks	ccsSNRRequestStoppedTime
		description 	"The value of sysUpTime when the CNR measurement
        operation stops."
		characteristics "-N ccsSNRRequestStoppedTime -O 1.3.6.1.4.1.9.9.114.1.2.3.1.7 -T TIMETICKS -A RO -X equal -F 0"

		readwrite enum ccsSNRRequestStatusE	ccsSNRRequestStatus
		description 	"The control that allows modification, creation, and
        deletion of entries.  For detailed rules see the
        ccsSpectrumRequestEntry DESCRIPTION."
		characteristics "-N ccsSNRRequestStatus -O 1.3.6.1.4.1.9.9.114.1.2.3.1.8 -T INTEGER -A RW -X equal -F 0"

	)

