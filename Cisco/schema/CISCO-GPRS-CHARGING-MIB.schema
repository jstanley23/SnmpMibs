	(
		notaccessible gauge	cgprsCgTariffTimeIndex
		description 	"An arbitrary non-zero integer value uniquely identifying
        each charging tariff time."
		characteristics "-N cgprsCgTariffTimeIndex -O 1.3.6.1.4.1.9.9.192.1.1.21.1.1 -T GAUGE -A NA -X equal -F 0"

		readwrite enum cgprsCgTariffTimeRowStatusE	cgprsCgTariffTimeRowStatus
		description 	"This object is used to create a new row or delete an
        existing row in this table. 
        The objects in this table can't be modified if the value 
        of this object is 'active'.RowStatus options 'notInService' 
        and 'createAndWait' are not supported."
		characteristics "-N cgprsCgTariffTimeRowStatus -O 1.3.6.1.4.1.9.9.192.1.1.21.1.2 -T INTEGER -A RW -X equal -F 0"

		readwrite gauge	cgprsCgTariffTimeHour
		description 	"Specifies the hours part of GPRS tariffs time."
		characteristics "-N cgprsCgTariffTimeHour -O 1.3.6.1.4.1.9.9.192.1.1.21.1.3 -T GAUGE -A RW -X equal -F 0"

		readwrite gauge	cgprsCgTariffTimeMin
		description 	"Specifies the minutes part of GPRS tariffs time."
		characteristics "-N cgprsCgTariffTimeMin -O 1.3.6.1.4.1.9.9.192.1.1.21.1.4 -T GAUGE -A RW -X equal -F 0"

		readwrite gauge	cgprsCgTariffTimeSec
		description 	"Specifies the seconds part of GPRS tariffs time."
		characteristics "-N cgprsCgTariffTimeSec -O 1.3.6.1.4.1.9.9.192.1.1.21.1.5 -T GAUGE -A RW -X equal -F 0"

	)

	table	cgprsCgGatewayTable
	description "Charging gateway table.
        The table contains a list of charging gateways.
        A charging gateway contains databases to store the 
        collected G-CDRs for billing purposes.
        cgprsCgGatewayTable only stores information about
        the global default charging gateway group. However, either 
        of global default charing gateway or charging gateway 1-29
        can be used under an APN.
        This table is deprecated by cgprsCgGatewayIpTable."
	characteristics "-K ONE"
	(
		notaccessible int	cgprsCgGatewayIndex
		description 	"An unique identifier for the CG."
		characteristics "-N cgprsCgGatewayIndex -O 1.3.6.1.4.1.9.9.192.1.1.22.1.1 -T INTEGER -A NA -X equal -F 0"

		readwrite enum cgprsCgGatewayAddrTypeE	cgprsCgGatewayAddrType
		description 	"This object indicates the type of Internet address
        given in cgprsCgGatewayAddr."
		characteristics "-N cgprsCgGatewayAddrType -O 1.3.6.1.4.1.9.9.192.1.1.22.1.2 -T INTEGER -A RW -X equal -F 0"

		readwrite octet[255]	cgprsCgGatewayAddr
		description 	"The IP address that is used to uniquely identify the CG."
		characteristics "-N cgprsCgGatewayAddr -O 1.3.6.1.4.1.9.9.192.1.1.22.1.3 -T STRING -A RW -X equal -F 0"

		readwrite enum cgprsCgGatewayRowStatusE	cgprsCgGatewayRowStatus
		description 	"This object is used to create a new row or delete an
        existing row in this table.
        
        To create a row, set this object to 'createAndGo'.
        cgprsCgGatewayAddrType and cgprsCgGatewayAddr are the
        mandatory objects in a create request.
        
        To delete a row, set this object to 'destroy'."
		characteristics "-N cgprsCgGatewayRowStatus -O 1.3.6.1.4.1.9.9.192.1.1.22.1.4 -T INTEGER -A RW -X equal -F 0"

		readonly enum cgprsCgOperStatusE	cgprsCgOperStatus
		description 	"This object is used to indicate whether the configured CG
        is active(2) or standby(3).The object will return undefined(1) 
        when its not used either as active(2) or standby(3)."
		characteristics "-N cgprsCgOperStatus -O 1.3.6.1.4.1.9.9.192.1.1.22.1.5 -T INTEGER -A RO -X equal -F 0"

		readonly enum cgprsCgLinkStateE	cgprsCgLinkState
		description 	"This object is used to indicate the reachability of the CG.
        If the CG is reachable then this object is set to 'on'. When
        the CG is not reachable but the GGSN tries to establish a 
        connection then the object will be set to 'pending' state. 
        When the CG is not reachable then the value returned for this 
        object will be 'off'."
		characteristics "-N cgprsCgLinkState -O 1.3.6.1.4.1.9.9.192.1.1.22.1.6 -T INTEGER -A RO -X equal -F 0"

	)

	table	cgprsCgProfileTable
	description "Charging characteristics is a parameter introduced in
        Release 99.Charging Characteristics allows the operator 
        to apply different charging methods on a per PDP basis.
        
        Charging Characteristics is a 2-octet parameter:
        
        The first 4 bits of the first octet, the 'P' bits, is called 
        the charging profile index. It specifies up to 16 different 
        charging profiles. 
        
        The remaining bits, the 'B' bits, can be used to further 
        characterize the Charging behavior. 
        
        Charging characteristics can be received from the SGSN as an 
        IE in the create PDP request message or from locally defined 
        defaults. There can be local default at the access-point or 
        GGSN levels. At either level, there may be also different 
        defaults for home, roaming, visiting subscribers, etc.
        
        This table allows to create a charging profile and define
        all the characteristics for that profile. This profile
        will be mapped to some APN."
	characteristics "-K ONE"
	(
		notaccessible gauge	cgprsCgProfileNum
		description 	"This object is the charging profile index. This object
        represents the 'P' bits in charging characteristics."
		characteristics "-N cgprsCgProfileNum -O 1.3.6.1.4.1.9.9.192.1.1.30.1.1 -T GAUGE -A NA -X equal -F 0"

		readwrite enum cgprsCgProfileRowStatusE	cgprsCgProfileRowStatus
		description 	"This object is used to create a new row or delete an
        existing row in this table.
        
        To create a row, set this object to 'createAndGo'.
        
        To delete a row, set this object to 'destroy'.
        
        Only these two values 'createAndGo' and 'destroy'
        will be accepted.  The table objects can be
        modified, when the RowStatus (cgprsCgProfileRowStatus) is 
        active."
		characteristics "-N cgprsCgProfileRowStatus -O 1.3.6.1.4.1.9.9.192.1.1.30.1.2 -T INTEGER -A RW -X equal -F 0"

		readwrite octet[99]	cgprsCgProfileDesc
		description 	"This object is used to describe this profile."
		characteristics "-N cgprsCgProfileDesc -O 1.3.6.1.4.1.9.9.192.1.1.30.1.3 -T STRING -A RW -X equal -F 0"

		readwrite enum cgprsCgProfileCategoryE	cgprsCgProfileCategory
		description 	"This object defines the charging profile category.
        
        hotRate(0) - Charging by hot billing.
        flat(1)    - Flat rate charging.
        prepaid(2) - Prepaid charging.
        normal(3)  - Normal charging."
		characteristics "-N cgprsCgProfileCategory -O 1.3.6.1.4.1.9.9.192.1.1.30.1.4 -T INTEGER -A RW -X equal -F 0"

		readwrite enum cgprsCgProfileCdrSuppressE	cgprsCgProfileCdrSuppress
		description 	"This object specifies the generation of CDRs.
        If set to 'true', there won't be any CDRs generated.
        If set to 'false', the CDRs will be generated."
		characteristics "-N cgprsCgProfileCdrSuppress -O 1.3.6.1.4.1.9.9.192.1.1.30.1.5 -T INTEGER -A RW -X equal -F 0"

		readwrite gauge	cgprsCgProfileVolumeLimit
		description 	"This object specifies the volume limit at which the
        CDR updation has to take place."
		characteristics "-N cgprsCgProfileVolumeLimit -O 1.3.6.1.4.1.9.9.192.1.1.30.1.6 -T GAUGE -A RW -X equal -F 0"

		readwrite enum cgprsCgProfileVolumeLimitResetE	cgprsCgProfileVolumeLimitReset
		description 	"This object is used to reset the volume limit timer
        specified in cgprsCgProfileVolumeLimit when another
        trigger occurs. 
        
        If set to 'true' the volume limit timer will be reset when
        another trigger such as duration limit occurs.
        
        If set to 'false', the volume timer will continue until 
        it reaches the value set in cgprsCgProfileVolumeLimit."
		characteristics "-N cgprsCgProfileVolumeLimitReset -O 1.3.6.1.4.1.9.9.192.1.1.30.1.7 -T INTEGER -A RW -X equal -F 0"

		readwrite gauge	cgprsCgProfileDurLimit
		description 	"This object specifies the time limit in which the
        CDR updation has to take place.
        
        The value '0' means no duration limit."
		characteristics "-N cgprsCgProfileDurLimit -O 1.3.6.1.4.1.9.9.192.1.1.30.1.8 -T GAUGE -A RW -X equal -F 0"

		readwrite enum cgprsCgProfileDurLimitResetE	cgprsCgProfileDurLimitReset
		description 	"This object is used to reset the duration limit timer
        specified in cgprsCgProfileDurLimit when another
        trigger occurs.
        
        If set to 'true' the duration limit timer will be reset when
        another trigger such as volume limit occurs.
        
        If set to 'false', the duration timer will continue until 
        it reaches the value set in cgprsCgProfileDurLimit."
		characteristics "-N cgprsCgProfileDurLimitReset -O 1.3.6.1.4.1.9.9.192.1.1.30.1.9 -T INTEGER -A RW -X equal -F 0"

		readwrite enum cgprsCgProfileTariffTimeE	cgprsCgProfileTariffTime
		description 	"This object enables the tariff time for this profile."
		characteristics "-N cgprsCgProfileTariffTime -O 1.3.6.1.4.1.9.9.192.1.1.30.1.10 -T INTEGER -A RW -X equal -F 0"

		readwrite int	cgprsCgProfileSgsnChange
		description 	"This object denotes the maximum number of SGSN changes that
        will trigger the closure of G-CDR or partial record. When the
        maximum number is reached, the record is closed and a new
        partial record is opened.  When the value is zero, then for
        every SGSN change, the G-CDR or partial record is closed.
        
        The value '-1' indicates no sgsn change limit."
		characteristics "-N cgprsCgProfileSgsnChange -O 1.3.6.1.4.1.9.9.192.1.1.30.1.11 -T INTEGER -A RW -X equal -F 0"

		readwrite enum cgprsCgProfileCdrSuppressPrepaidE	cgprsCgProfileCdrSuppressPrepaid
		description 	"This object specifies the suppression of CDRs for
        prepaid.
        
        This object can be set to 'true' only when the object
        cgprsCgProfileCdrSuppress is set to 'false'.
        
        If set to 'true', there won't be any CDRs generated
        for prepaid users.
        
        If set to 'false', the CDRs will be generated for
        prepaid users."
		characteristics "-N cgprsCgProfileCdrSuppressPrepaid -O 1.3.6.1.4.1.9.9.192.1.1.30.1.12 -T INTEGER -A RW -X equal -F 0"

		readwrite octet[255]	cgprsCgProfileContentDccaProfile
		description 	"This object specifies a DCCA client to use to communicate
        with a DCCA server in a GGSN charging profile.
        
        The profile name is the DCCA profile name 
        (cGgsnSADccaProfileName) which is present in  
        CISCO-GGSN-SERVICE-AWARE-MIB and it's outside the scope of 
        this MIB. 
        
        The presence of the object in this table indicates online 
        billing is required. Therefore, regardless of whether a 
        subscriber is prepaid or postpaid, the GGSN will contact 
        the DCCA server if this object is set.
        
        If the user is to be treated as a postpaid user, the server 
        returns X and the user is treated as a postpaid user. If 
        this object is not set to valid value, then users using the 
        charging profile will be treated as postpaid (offline 
        billing)."
		characteristics "-N cgprsCgProfileContentDccaProfile -O 1.3.6.1.4.1.9.9.192.1.1.30.1.13 -T STRING -A RW -X equal -F 0"

		readwrite gauge	cgprsCgProfileContentPostTime
		description 	"This object specifies a trigger condition for postpaid
        users in this charging profile, the time duration limit 
        that when exceeded causes the GGSN to collect upstream 
        and downstream traffic byte counts and close and update 
        the G-CDR for a particular PDP context."
		characteristics "-N cgprsCgProfileContentPostTime -O 1.3.6.1.4.1.9.9.192.1.1.30.1.14 -T GAUGE -A RW -X equal -F 0"

		readwrite gauge	cgprsCgProfileContentPostValTime
		description 	"This object specifies the validity timeout for this
        postpaid users in this charging profile. By default, the
        time and volume quota for postpaid has no validity timeout.
        If it is configured, it applies to both time based or volume 
        based quota.  This object is deprecated by 
        cgprsCgProfileContentPostValidityTime"
		characteristics "-N cgprsCgProfileContentPostValTime -O 1.3.6.1.4.1.9.9.192.1.1.30.1.15 -T GAUGE -A RW -X equal -F 0"

		readwrite gauge	cgprsCgProfileContentPostVolume
		description 	"This object specifies a trigger condition for postpaid
        users in this charging profile, the maximum number of 
        bytes that the GGSN maintains across all containers for 
        a particular PDP context before closing and updating 
        the G-CDR."
		characteristics "-N cgprsCgProfileContentPostVolume -O 1.3.6.1.4.1.9.9.192.1.1.30.1.16 -T GAUGE -A RW -X equal -F 0"

		readwrite octet[255]	cgprsCgProfileContentRulebaseId
		description 	"This object sets the rulebase ID for this charging profile.
        
        Rulebases contain the rules for defining categories of 
        traffic, categories on which decisions such as whether to 
        allow or disallow traffic, and how to measure the traffic, 
        are based. The GGSN maps Diameter Rulebase IDs to CSG 
        billing plans.
        
        The rulebase value presented in a RADIUS Access Accept 
        message overrides the default rulebase ID configured in 
        the charging profile. A rulebase ID received in a CCA 
        initial message from a DCCA server overrides the Rulebase 
        ID received from the RADIUS server and the default rulebase 
        ID configured in a charging profile."
		characteristics "-N cgprsCgProfileContentRulebaseId -O 1.3.6.1.4.1.9.9.192.1.1.30.1.17 -T STRING -A RW -X equal -F 0"

		readwrite enum cgprsCgProfileContentPostQosChangeE	cgprsCgProfileContentPostQosChange
		description 	"This object when set to 'true' enables the trigger for QoS
        change for postpaid users using this charging profile. 
        
        The change in QoS causes GGSN to close the G-CDR for a 
        particular PDP context and generate the service record for each
        service."
		characteristics "-N cgprsCgProfileContentPostQosChange -O 1.3.6.1.4.1.9.9.192.1.1.30.1.18 -T INTEGER -A RW -X equal -F 0"

		readwrite enum cgprsCgProfileContentPostSgsnChangeE	cgprsCgProfileContentPostSgsnChange
		description 	"This object when set to 'true' enables the trigger for SGSN
        change for postpaid users using this charging profile.  The
        SGSN address is added to the SGSN list in the context G-CDR.
        
        When the SGSN limit is reached, the G-CDR is closed. Current
        SGSN address is written into the CDR, when the cgprsCgRelease  
        value is 'release4' or above, and service record is 
        generated."
		characteristics "-N cgprsCgProfileContentPostSgsnChange -O 1.3.6.1.4.1.9.9.192.1.1.30.1.19 -T INTEGER -A RW -X equal -F 0"

		readwrite enum cgprsCgProfileContentPostRatChangeE	cgprsCgProfileContentPostRatChange
		description 	"This object when set to 'true' enables the trigger for RAT
        change for postpaid users using this charging profile.
        
        This object can be set to 'true' only when the object 
        cgprsCgServiceRecordIncludeRat is 'true'.
        
        When a RAT change is received, the G-CDR is closed. The new 
        RAT value is written into the new G-CDR and service record is
        is generated with the new RAT value."
		characteristics "-N cgprsCgProfileContentPostRatChange -O 1.3.6.1.4.1.9.9.192.1.1.30.1.20 -T INTEGER -A RW -X equal -F 0"

		readwrite enum cgprsCgProfileContentPostPlmnChangeE	cgprsCgProfileContentPostPlmnChange
		description 	"This object when set to 'true' enables the trigger for PLMN
        change for postpaid users using this charging profile.  When 
        enabled, the current PLMN-ID is always written into the 
        service-record.  If none is received in the PDP create/update 
        request, a value of zero is written into the service-record.
        
        This object can be set to 'true' only when the object 
        cgprsCgServiceRecordIncludePlmn is 'true'.
        
        When a PLMN-ID change is received, the G-CDR is closed. 
        A new service-record is generated and the new PLMN-ID is 
        written into the new service-record."
		characteristics "-N cgprsCgProfileContentPostPlmnChange -O 1.3.6.1.4.1.9.9.192.1.1.30.1.21 -T INTEGER -A RW -X equal -F 0"

		readwrite enum cgprsCgProfileStorageTypeE	cgprsCgProfileStorageType
		description 	"The storage type for this conceptual row.
        Conceptual rows having the value 'permanent' need not
        allow write-access to any columnar objects in the row."
		characteristics "-N cgprsCgProfileStorageType -O 1.3.6.1.4.1.9.9.192.1.1.30.1.22 -T INTEGER -A RW -X equal -F 0"

		readwrite gauge	cgprsCgProfileContentPostValidityTime
		description 	"This object specifies the validity timeout for this
        postpaid users in this charging profile. By default, the
        time and volume quota for postpaid has no validity timeout.
        If it is configured, it applies to both time based or volume
        based quota.  The valid range of this object is from 900 to
        4294967295, and the value 0 indicates that the validity
        timeout is not configured."
		characteristics "-N cgprsCgProfileContentPostValidityTime -O 1.3.6.1.4.1.9.9.192.1.1.30.1.23 -T GAUGE -A RW -X equal -F 0"

		readwrite enum cgprsCgProfileContentPostUserLocChangeE	cgprsCgProfileContentPostUserLocChange
		description 	"This object when set to 'true' enables the trigger for user
        location change for postpaid users using this charging profile.
        When the trigger is enabled and a user location change is
        received, the G-CDR is closed. A new service-record is generated
        and the new user location information is written into the new
        service-record.
        
        This object can be set to 'true' only when the object
        cgprsCgServiceRecordIncludeUserLocInfo is 'true'."
		characteristics "-N cgprsCgProfileContentPostUserLocChange -O 1.3.6.1.4.1.9.9.192.1.1.30.1.24 -T INTEGER -A RW -X equal -F 0"

		readwrite enum cgprsCgProfileScdrEnableE	cgprsCgProfileScdrEnable
		description 	"This object specifies whether the SGW CDR
        generation is enabled or disabled, for the sessions
        in SPGW mode.
        When set to 'true' SGW mode CDR generation is enabled.
        when set to 'false' SGW mode CDR generation is Disabled.
        By default the value of this object is 'false'."
		characteristics "-N cgprsCgProfileScdrEnable -O 1.3.6.1.4.1.9.9.192.1.1.30.1.25 -T INTEGER -A RW -X equal -F 0"

	)

	table	cgprsCgProfileMapTable
	description "This table maintains the default charging profile
        mappings. The profile set in cgprsCgProfileTable will
        be mapped to the default profile table. Only one
        mapping to the type of MS can be present.
        
        This is a static table which contains only four entries,
        each for a category."
	characteristics "-K ONE"
	(
		notaccessible enum cgprsCgProfileMapCategoryE	cgprsCgProfileMapCategory
		description 	"This object defines the category in which the MS
        falls into."
		characteristics "-N cgprsCgProfileMapCategory -O 1.3.6.1.4.1.9.9.192.1.1.31.1.1 -T INTEGER -A NA -X equal -F 0"

		readwrite int	cgprsCgProfileMapNum
		description 	"This object represents the profile number which is
        created using cgprsCgProfileTable. This object holds the
        valid value represented by cgprsCgProfileNum. The profile 
        number 0 cannot be created or deleted using 
        cgprsCgProfileTable. It's created using the other 
        configuration objects cgprsCgContainerVolThresh, 
        cgprsCgCdrSgsnChangeLimit, cgprsCgContainerTimeLimit.
        This object is deprecated by cgprsCgProfileMapNumber."
		characteristics "-N cgprsCgProfileMapNum -O 1.3.6.1.4.1.9.9.192.1.1.31.1.2 -T INTEGER -A RW -X equal -F 0"

		readwrite enum cgprsCgProfileMapOverrideE	cgprsCgProfileMapOverride
		description 	"This object defines whether the profile set in
        cgprsCgProfileMapNum should override the value received 
        from SGSN."
		characteristics "-N cgprsCgProfileMapOverride -O 1.3.6.1.4.1.9.9.192.1.1.31.1.3 -T INTEGER -A RW -X equal -F 0"

		readwrite enum cgprsCgProfileMapTrustedE	cgprsCgProfileMapTrusted
		description 	"This object defines whether the profile for 'roaming',
        'visitor' is a trusted. This object has no meaning when
        the value of the cgprsCgProfileMapCategory is 'all' or
        'home'.
        
        If this object is set to 'true' and cgprsCgProfileMapCategory
        is 'roaming', then this profile will be the default profile
        for trusted roamer. If set to 'false', then it's just a
        roamer.
        
        If this object is set to 'true' and cgprsCgProfileMapCategory
        is 'visitor', then this profile will be the default profile
        for trusted visitor. If set to 'false', then it's just a
        roamer."
		characteristics "-N cgprsCgProfileMapTrusted -O 1.3.6.1.4.1.9.9.192.1.1.31.1.4 -T INTEGER -A RW -X equal -F 0"

		readwrite int	cgprsCgProfileMapNumber
		description 	"This object represents the profile number which is
        created using cgprsCgProfileTable. This object holds the
        valid value represented by cgprsCgProfileNum. The profile
        number 0 cannot be created or deleted using
        cgprsCgProfileTable. It's created using the other
        configuration objects cgprsCgContainerVolThresh,
        cgprsCgCdrSgsnChangeLimit, cgprsCgContainerTimeLimit.
        The value -1 for this object indicates that the default
        profile for the category is not configured."
		characteristics "-N cgprsCgProfileMapNumber -O 1.3.6.1.4.1.9.9.192.1.1.31.1.5 -T INTEGER -A RW -X equal -F 0"

	)

	table	cgprsCgGroupTable
	description "Charging gateway group table.
        The table contains a list of charging groups.
        In each charging group, a unique primary, secondary, and
        tertiary charging gateway, and iSCSI target can be defined
        and associated with an APN. Charging group enables sending
        charging records belonging to different APNs to 
        different destinations."
	characteristics "-K ONE"
	(
		notaccessible int	cgprsCgGroupIndex
		description 	"A locally unique identifier for the charging groups
        on GGSN.
        Note: There is support for only 30 charging 
        groups (0-29). Where charging group 0 is also 
        referred as default charging gateway group."
		characteristics "-N cgprsCgGroupIndex -O 1.3.6.1.4.1.9.9.192.1.1.60.1.1 -T INTEGER -A NA -X equal -F 0"

		readwrite octet[255]	cgprsCgGroupIscsi
		description 	"This object specifies the iSCSI gateway name for
        the charging group."
		characteristics "-N cgprsCgGroupIscsi -O 1.3.6.1.4.1.9.9.192.1.1.60.1.2 -T STRING -A RW -X equal -F 0"

		readwrite enum cgprsCgGroupServiceModeE	cgprsCgGroupServiceMode
		description 	"This object specifies the charging service-mode for a charging
        group. The charging functions in the GGSN involve mainly
        collection/accumulation of CDRs and transmitting CDRs to the
        Charging Gateways. The charging service-mode function has no
        impact to the collection/accumulation of CDRs. The charging
        service mode function only involves the transmission of CDRs
        to the charging gateways. The charging service-mode has the
        following two states:
        'operational' : In this state, the charging group will observe 
                        normal charging operations. That is,
                        accumulation and transmission of CDRs to the 
                        charging gateway will continue as is done 
                        normally.
        'maintenance' : In this state, transmission of CDRs to the
                        charging gateways will not be performed;
                        However, collection and accumulation of CDRs
                        will continue as is done normally.
        
        When the GGSN is in 'maintenance' mode, all the charging
        configurations will be allowed. In the system-init phase,
        the charging service mode CLI configs will not be handled.
        
        The handling of 'Redirecting Request', 'Node Alive' and
        charging gateway switchover mechanisms will not be performed
        while the charging is in maintenance mode. After the mode is
        changed to operational mode, the messages in the pending queue 
        will be sent towards the newly configured active charging 
        gateway and the normal functions will continue from thereon.
        When switching between modes, traps will be generated using
        cgprsCgGatewayGroupInServiceModeNotif and 
        cgprsCgGatewayGroupMaintenanceModeNotif."
		characteristics "-N cgprsCgGroupServiceMode -O 1.3.6.1.4.1.9.9.192.1.1.60.1.3 -T INTEGER -A RW -X equal -F 0"

		readwrite enum cgprsCgGroupSwitchOverPriorityE	cgprsCgGroupSwitchOverPriority
		description 	"This object specifies the switch over priority
        mechanism.
        
        'true'  - If a higher priority CG comes up, it becomes the
                  active CG.
        
        'false' - If a higher priority CG comes up, the existing
                  active CG continues to be active."
		characteristics "-N cgprsCgGroupSwitchOverPriority -O 1.3.6.1.4.1.9.9.192.1.1.60.1.4 -T INTEGER -A RW -X equal -F 0"

		readwrite enum cgprsCgGroupRowStatusE	cgprsCgGroupRowStatus
		description 	"This object is used to create a new row or delete
        an existing row in this table."
		characteristics "-N cgprsCgGroupRowStatus -O 1.3.6.1.4.1.9.9.192.1.1.60.1.5 -T INTEGER -A RW -X equal -F 0"

	)

	table	cgprsCgGatewayIpTable
	description "Charging gateway table.
        The table contains a list of charging gateways.
        A charging gateway contains databases to store the 
        collected G-CDRs for billing purposes."
	characteristics "-K ONE"
	(
		notaccessible gauge	cgprsCgGatewayIpIndex
		description 	"An unique identifier for the CG."
		characteristics "-N cgprsCgGatewayIpIndex -O 1.3.6.1.4.1.9.9.192.1.1.61.1.1 -T GAUGE -A NA -X equal -F 0"

		readwrite enum cgprsCgGatewayIpAddrTypeE	cgprsCgGatewayIpAddrType
		description 	"This object specifies the type of Internet address
        given in cgprsCgGatewayIpAddr."
		characteristics "-N cgprsCgGatewayIpAddrType -O 1.3.6.1.4.1.9.9.192.1.1.61.1.2 -T INTEGER -A RW -X equal -F 0"

		readwrite octet[255]	cgprsCgGatewayIpAddr
		description 	"This object specifies the IP address that is used to uniquely
        identify the CG."
		characteristics "-N cgprsCgGatewayIpAddr -O 1.3.6.1.4.1.9.9.192.1.1.61.1.3 -T STRING -A RW -X equal -F 0"

		readonly enum cgprsCgGatewayOperStatusE	cgprsCgGatewayOperStatus
		description 	"This object is used to indicate what is the operation status
        of the configured charging gateway.
        
        undefined(1)    The operation status of the CG is neither 
                        active nor standby. 
        active(2)       The operation status of the CG is active
        standby(3)      The operation status of the CG is standby"
		characteristics "-N cgprsCgGatewayOperStatus -O 1.3.6.1.4.1.9.9.192.1.1.61.1.4 -T INTEGER -A RO -X equal -F 0"

		readonly enum cgprsCgGatewayLinkStateE	cgprsCgGatewayLinkState
		description 	"This object is used to indicate the reachability of the CG.
        
        on(1)         The CG is reachable
        off(2)        The CG is not reachable
        pending(3)    The CG is not reachable but the GGSN is trying to
                      establish a connection"
		characteristics "-N cgprsCgGatewayLinkState -O 1.3.6.1.4.1.9.9.192.1.1.61.1.5 -T INTEGER -A RO -X equal -F 0"

		readonly enum cgprsCgGatewayE	cgprsCgGateway
		description 	"This object indicates the type of GPRS charging gateway."
		characteristics "-N cgprsCgGateway -O 1.3.6.1.4.1.9.9.192.1.1.61.1.6 -T INTEGER -A RO -X equal -F 0"

		readwrite enum cgprsCgGatewayIpRowStatusE	cgprsCgGatewayIpRowStatus
		description 	"This object is used to create a new row or delete an
        existing row in this table."
		characteristics "-N cgprsCgGatewayIpRowStatus -O 1.3.6.1.4.1.9.9.192.1.1.61.1.7 -T INTEGER -A RW -X equal -F 0"

	)

