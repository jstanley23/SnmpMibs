		characteristics "-N ciapPrefixThresholdFalling -O 1.3.6.1.4.1.9.9.748.1.2.6.1.12 -T GAUGE -A RW -X equal -F 0"

		readwrite gauge	ciapPrefixPrefixesInUse
		description 	"This object indicates the number of IP prefixes allocated from
