	(
		notaccessible octet[6]	mplsLdpPeerLdpId
		description 	"The LDP identifier of this LDP Peer."
		characteristics "-N mplsLdpPeerLdpId -O 1.3.6.1.2.1.10.166.4.1.3.2.1.1 -T STRING -A NA -X equal -F 0"

		readonly enum mplsLdpPeerLabelDistMethodE	mplsLdpPeerLabelDistMethod
		description 	"For any given LDP session, the method of
        label distribution must be specified."
