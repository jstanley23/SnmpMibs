	table	alpsAscuTable
	description "A list of ALPS ASCUs on an interface. This is defined only at
        the remote router."
	characteristics "-K ONE"
	(
		notaccessible int	alpsAscuId
		description 	"Specifies the ASCU Identifier and uniquely
        identifies an ASCU on a given interface. This value is
        the Interface Address (IA) for ALC and the Remote Identifier
        (RID) for UTS. This uniquely identifies an ASCU on a given interface."
		characteristics "-N alpsAscuId -O 1.3.6.1.4.1.9.9.95.1.5.1.1.1 -T INTEGER -A NA -X equal -F 0"

		readwrite int	alpsAscuA1
		description 	"A1 value of the ASCU."
		characteristics "-N alpsAscuA1 -O 1.3.6.1.4.1.9.9.95.1.5.1.1.2 -T INTEGER -A RW -X equal -F 0"

		readwrite int	alpsAscuA2
		description 	"A2 value of the ASCU."
		characteristics "-N alpsAscuA2 -O 1.3.6.1.4.1.9.9.95.1.5.1.1.3 -T INTEGER -A RW -X equal -F 0"

		readwrite octet[12]	alpsAscuCktName
		description 	"Specifies the ALPS Circuit Name for the circuit that is to be
        used for this ASCU's data. Note that all ASCUs on a physical interface
        may not use the same ALPS circuit to connect to the central router"
		characteristics "-N alpsAscuCktName -O 1.3.6.1.4.1.9.9.95.1.5.1.1.4 -T STRING -A RW -X equal -F 0"

		readwrite enum alpsAscuAlarmsEnabledE	alpsAscuAlarmsEnabled
		description 	"Specifies whether alarms (syslog messages or SNMP traps) will
        be generated for this ASCU."
		characteristics "-N alpsAscuAlarmsEnabled -O 1.3.6.1.4.1.9.9.95.1.5.1.1.5 -T INTEGER -A RW -X equal -F 0"

		readwrite enum alpsAscuRetryOptionE	alpsAscuRetryOption
		description 	"Specifies the retry option for this ASCU. This specifies the
        error message to be sent to the terminal when a message
        is dropped at the router. The terminal has three choices -
        either retransmit the last message (if the terminal can buffer it),
        re-enter the last message sent or do nothing. Based on the terminal's
        capabilities, one of these three is configured for the ASCU. Note that
        this option is the same for all terminals connected to an ASCU
        because the ALPS feature does not handle individual terminals"
		characteristics "-N alpsAscuRetryOption -O 1.3.6.1.4.1.9.9.95.1.5.1.1.6 -T INTEGER -A RW -X equal -F 0"

		readwrite int	alpsAscuMaxMsgLength
		description 	"Specifies Maximum length of Input Message that can be received
        from this ASCU."
		characteristics "-N alpsAscuMaxMsgLength -O 1.3.6.1.4.1.9.9.95.1.5.1.1.7 -T INTEGER -A RW -X equal -F 0"

		readwrite enum alpsAscuFwdStatusOptionE	alpsAscuFwdStatusOption
		description 	"If this flag is set, status messages received from the ASCU are
        forwarded to the host. Otherwise they are discarded. Note that these
        are relevant only for P1024C (UTS). For ALC, a value of FALSE will 
        be returned."
		characteristics "-N alpsAscuFwdStatusOption -O 1.3.6.1.4.1.9.9.95.1.5.1.1.8 -T INTEGER -A RW -X equal -F 0"

		readonly enum alpsAscuStateE	alpsAscuState
		description 	"Reports the current ASCU Status. ASCUs are initially in
        disabled state. They are enabled from the parser
        if the operator wants to. This takes them to an UP state from which 
        they go to a DOWN state if there are too many errors."
		characteristics "-N alpsAscuState -O 1.3.6.1.4.1.9.9.95.1.5.1.1.9 -T INTEGER -A RO -X equal -F 0"

		readonly enum alpsAscuDownReasonE	alpsAscuDownReason
		description 	"Reports the Down reason for this ASCU if it is down."
		characteristics "-N alpsAscuDownReason -O 1.3.6.1.4.1.9.9.95.1.5.1.1.10 -T INTEGER -A RO -X equal -F 0"

		readonly counter	alpsAscuOutPackets
		description 	"Number of packets transmitted to this ASCU."
		characteristics "-N alpsAscuOutPackets -O 1.3.6.1.4.1.9.9.95.1.5.1.1.11 -T COUNTER -A RO -X equal -F 0"

		readonly counter	alpsAscuOutOctets
		description 	"Number of octets transmitted to this ASCU."
		characteristics "-N alpsAscuOutOctets -O 1.3.6.1.4.1.9.9.95.1.5.1.1.12 -T COUNTER -A RO -X equal -F 0"

		readonly counter	alpsAscuInPackets
		description 	"Number of packets received from this ASCU"
		characteristics "-N alpsAscuInPackets -O 1.3.6.1.4.1.9.9.95.1.5.1.1.13 -T COUNTER -A RO -X equal -F 0"

		readonly counter	alpsAscuInOctets
		description 	"Number of octets received from this ASCU."
		characteristics "-N alpsAscuInOctets -O 1.3.6.1.4.1.9.9.95.1.5.1.1.14 -T COUNTER -A RO -X equal -F 0"

		readonly counter	alpsAscuDropsGarbledPkts
		description 	"The number of packets dropped due to the inability to properly
        decode them. Examples of garbled packets include packets with bad
        CRCs or bad header information"
		characteristics "-N alpsAscuDropsGarbledPkts -O 1.3.6.1.4.1.9.9.95.1.5.1.1.15 -T COUNTER -A RO -X equal -F 0"

		readonly counter	alpsAscuDropsAscuDown
		description 	"The number of packets dropped because the ASCU had previously
        been declared DOWN due to error thresholds being exceeded."
		characteristics "-N alpsAscuDropsAscuDown -O 1.3.6.1.4.1.9.9.95.1.5.1.1.16 -T COUNTER -A RO -X equal -F 0"

		readonly counter	alpsAscuDropsAscuDisabled
		description 	"The number of packets dropped because the ASCU had previously
        been declared to be in disabled state."
		characteristics "-N alpsAscuDropsAscuDisabled -O 1.3.6.1.4.1.9.9.95.1.5.1.1.17 -T COUNTER -A RO -X equal -F 0"

		readwrite enum alpsAscuEnabledE	alpsAscuEnabled
		description 	"Specifies whether the ASCU has been enabled or not. After
        the ASCU has been configured, it must be explicitly enabled to allow 
        it to be polled."
		characteristics "-N alpsAscuEnabled -O 1.3.6.1.4.1.9.9.95.1.5.1.1.18 -T INTEGER -A RW -X equal -F 0"

		readwrite enum alpsAscuRowStatusE	alpsAscuRowStatus
		description 	"This object is used to create a new row or modify or
        delete an existing row in this table."
		characteristics "-N alpsAscuRowStatus -O 1.3.6.1.4.1.9.9.95.1.5.1.1.19 -T INTEGER -A RW -X equal -F 0"

		readwrite enum alpsAscuAutoResetE	alpsAscuAutoReset
		description 	"Specifies whether a reset command will be automatically issued
        to any non-responsive ALC ASCUs."
		characteristics "-N alpsAscuAutoReset -O 1.3.6.1.4.1.9.9.95.1.5.1.1.20 -T INTEGER -A RW -X equal -F 0"

	)

