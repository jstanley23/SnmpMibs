		readonly gauge	cbgpPeer2DeniedPrefixes
		description 	"This counter is incremented when a route prefix, which
        belongs to an address family, received on this
        connection is denied. It is initialized to zero when
        the connection is undergone a hard reset."
		characteristics "-N cbgpPeer2DeniedPrefixes -O 1.3.6.1.4.1.9.9.187.1.2.8.1.2 -T GAUGE -A RO -X equal -F 0"

		readwrite gauge	cbgpPeer2PrefixAdminLimit
		description 	"Max number of route prefixes accepted for an address
        family on this connection."
		characteristics "-N cbgpPeer2PrefixAdminLimit -O 1.3.6.1.4.1.9.9.187.1.2.8.1.3 -T GAUGE -A RW -X equal -F 0"

		readwrite gauge	cbgpPeer2PrefixThreshold
		description 	"Prefix threshold value (%) for an address family
        on this connection at which warning message stating
        the prefix count is crossed the threshold or
        corresponding SNMP notification is generated."
		characteristics "-N cbgpPeer2PrefixThreshold -O 1.3.6.1.4.1.9.9.187.1.2.8.1.4 -T GAUGE -A RW -X equal -F 0"

		readonly gauge	cbgpPeer2PrefixClearThreshold
		description 	"Prefix threshold value (%) for an address family
        on this connection at which SNMP clear notification
        is generated if prefix threshold notification is
        already generated."
		characteristics "-N cbgpPeer2PrefixClearThreshold -O 1.3.6.1.4.1.9.9.187.1.2.8.1.5 -T GAUGE -A RO -X equal -F 0"

		readonly gauge	cbgpPeer2AdvertisedPrefixes
		description 	"This counter is incremented when a route prefix,
        which belongs to an address family is advertised
        on this connection. It is initialized to zero when
        the connection is undergone a hard reset."
		characteristics "-N cbgpPeer2AdvertisedPrefixes -O 1.3.6.1.4.1.9.9.187.1.2.8.1.6 -T GAUGE -A RO -X equal -F 0"

		readonly gauge	cbgpPeer2SuppressedPrefixes
		description 	"This counter is incremented when a route prefix,
        which belongs to an address family is suppressed
        from being sent on this connection. It is
        initialized to zero when the connection is undergone
        a hard reset."
		characteristics "-N cbgpPeer2SuppressedPrefixes -O 1.3.6.1.4.1.9.9.187.1.2.8.1.7 -T GAUGE -A RO -X equal -F 0"

		readonly gauge	cbgpPeer2WithdrawnPrefixes
		description 	"This counter is incremented when a route prefix,
        which belongs to an address family, is withdrawn on
        this connection. It is initialized to zero when the
        connection is undergone a hard reset."
		characteristics "-N cbgpPeer2WithdrawnPrefixes -O 1.3.6.1.4.1.9.9.187.1.2.8.1.8 -T GAUGE -A RO -X equal -F 0"

	)

	group	cbgpGlobal
	description "(null)"
	characteristics "-K NONE"
	(
		readwrite octet[2]	cbgpNotifsEnable
		description 	"Indicates whether the specific notifications are
        enabled. 
        If notifsEnable(0) bit is set to 1,
        then the notifications defined in
        ciscoBgp4NotificationsGroup1 are enabled; 
        If notifsPeer2Enable(1) bit is set to 1,
