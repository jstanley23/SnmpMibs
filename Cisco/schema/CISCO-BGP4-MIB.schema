        ciscoBgp4Peer2NotificationsGroup are enabled."
		characteristics "-N cbgpNotifsEnable -O 1.3.6.1.4.1.9.9.187.1.3.1 -T STRING -A RW -X equal -F 0"

		readonly gauge	cbgpLocalAs
		description 	"The local autonomous system (AS) number."
		characteristics "-N cbgpLocalAs -O 1.3.6.1.4.1.9.9.187.1.3.2 -T GAUGE -A RO -X equal -F 0"

	)

