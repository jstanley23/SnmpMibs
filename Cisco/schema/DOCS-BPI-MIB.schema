	table	docsBpiIpMulticastMapTable
	description "This table describes the mapping of IP multicast address prefixes to
        multicast SIDs on each CMTS MAC interface."
	characteristics "-K FIVE"
	(
		notaccessible netaddress	docsBpiIpMulticastAddress
		description 	"This object represents the IP multicast address (prefix) to be
        mapped by this row, in conjunction with
        docsBpiIpMulticastPrefixLength."
		characteristics "-N docsBpiIpMulticastAddress -O 1.3.6.1.2.1.10.127.5.1.2.4.1.1.1 -T IPADDRESS -A NA -X equal -F 0"

		notaccessible int	docsBpiIpMulticastPrefixLength
		description 	"This object represents the IP multicast address prefix length
        for this row. The value of this object represents the length in
        bits of docsBpiIpMulticastAddress for multicast address
        comparisons, using big-endian ordering. An IP multicast address
        matches this row if the (docsBpiIpMulticastPrefixLength) most
        significant bits of the IP multicast address and of the
        (docsBpiIpMulticastAddress) are identical.
        This object is similar in usage to an IP address mask. The value
        0 corresponds to IP address mask 0.0.0.0, the value 1 corresponds
        to IP address mask 128.0.0.0, the value 8 corresponds to IP
        address mask 255.0.0.0, and the value 32 corresponds to IP
        address mask 255.255.255.255."
		characteristics "-N docsBpiIpMulticastPrefixLength -O 1.3.6.1.2.1.10.127.5.1.2.4.1.1.2 -T INTEGER -A NA -X equal -F 0"

		readwrite int	docsBpiIpMulticastServiceId
		description 	"This object represents the multicast SID to be used in this
        IP multicast address prefix mapping entry."
		characteristics "-N docsBpiIpMulticastServiceId -O 1.3.6.1.2.1.10.127.5.1.2.4.1.1.3 -T INTEGER -A RW -X equal -F 0"

		readwrite enum docsBpiIpMulticastMapControlE	docsBpiIpMulticastMapControl
		description 	"This object controls and reflects the IP multicast address prefix
        mapping entry. There is no restriction on the ability to change values
        in this row while the row is active."
		characteristics "-N docsBpiIpMulticastMapControl -O 1.3.6.1.2.1.10.127.5.1.2.4.1.1.4 -T INTEGER -A RW -X equal -F 0"

	)

