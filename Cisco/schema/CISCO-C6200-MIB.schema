	group	c62System
	description "(null)"
	characteristics "-K NONE"
	(
		readonly enum systemTypeE	systemType
		description 	"This type provides specific details about C6200 system type and
        what kind of network interface it uses."
		characteristics "-N systemType -O 1.3.6.1.4.1.9.10.26.1.1.1 -T INTEGER -A RO -X equal -F 0"

		readonly enum systemAlarmLevelE	systemAlarmLevel
		description 	"This reflects the alarm level of the C6200 system. It is the
        aggregation of the slot/port level alarms. Alarm levels
        supported at the system level are none, minor, major and
        critical.  Unknown has no meaning."
		characteristics "-N systemAlarmLevel -O 1.3.6.1.4.1.9.10.26.1.1.2 -T INTEGER -A RO -X equal -F 0"

		readonly counter	systemAlarmLevelChngCounter
		description 	"Counter indicating a change in the system alarm level due to
        change either at the slot or port alarm level. This will be
        incremented by the system whenever any new alarm is raised or
        cleared at slot or port level. This is needed as rising or
        clearing of an alarm at port or slot level may not cause
        systemAlarmLevel to change.  This counter, in such case helps
        manager application to recognize that alarm levels below system
        level has changed without causing change in the
        systemAlarmLevel. e.g. Say: System has two subscriber lines
        experiencing a minor alarm condition.  systemAlarmLevel will be
        a minor alarm in such case. Now if one of the port level minor
        alarms clear, a port alarm level would become OK but the system
        and slot alarm levels may stay as a minor alarm."
		characteristics "-N systemAlarmLevelChngCounter -O 1.3.6.1.4.1.9.10.26.1.1.3 -T COUNTER -A RO -X equal -F 0"

		readwrite enum systemResetE	systemReset
		description 	"This parameter allows user to reboot the system with system
        configuration from NVRAM (non-volatile memory). It can be used
        for following purposes:
          (1) Undo changes made to the system and not yet saved in the
              NVRAM.
          (2) Force the system to reboot from NVRAM configuration.
        This command disrupts service to all the subscribers associated
        with the system, so it should be use sparingly."
		characteristics "-N systemReset -O 1.3.6.1.4.1.9.10.26.1.1.4 -T INTEGER -A RW -X equal -F 0"

		readwrite enum systemSaveCnfgE	systemSaveCnfg
		description 	"This parameter allows user/operator to make system changes
        permanent. It causes system to write the system configuration on
        a non-volatile memory (NVRAM). There is no need to restart the
        system to make these changes effective."
		characteristics "-N systemSaveCnfg -O 1.3.6.1.4.1.9.10.26.1.1.5 -T INTEGER -A RW -X equal -F 0"

		readonly counter	systemProvChngCounter
		description 	"Counter indicating a card or subscriber provisioning change.
        This will be incremented by the system whenever any change is
        made. This is used to avoid unneccessary polling."
		characteristics "-N systemProvChngCounter -O 1.3.6.1.4.1.9.10.26.1.1.6 -T COUNTER -A RO -X equal -F 0"

		readonly enum systemHClockAlarmLevelE	systemHClockAlarmLevel
		description 	"This reflects the alarm level of the H-bus backplane clock.
        H-bus clock error results in critical alarm at the at system
        level (systemAlarmLevel = critical)."
		characteristics "-N systemHClockAlarmLevel -O 1.3.6.1.4.1.9.10.26.1.1.7 -T INTEGER -A RO -X equal -F 0"

		readonly enum systemFanAlarmLevelE	systemFanAlarmLevel
		description 	"This reflects the alarm level of the fans. Error for this
        parameter indicates failure of one or more fans. This error
        results in minor alarm at the system level (systemAlarmLevel =
