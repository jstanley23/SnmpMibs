		characteristics "-N cpimExtRPSetGroupMask -O 1.3.6.1.4.1.9.10.120.1.1.7.1.4 -T GAUGE -A NA -X equal -F 0"

		notaccessible enum cpimExtRPSetTypeE	cpimExtRPSetType
		description 	"This object indicates how the RP was learnt, 'static' means
        
        static   (2) indicates entry was learnt from local 
        configuration.
        
        bsr      (3) indicates entry was learnt from BSR RP discovery 
        protocol. 
        
        embedded (4) indicates  entry was learnt from Embedded-RP
        mechanism where the RP-Address is embedded in the Multicast
        Group address.
        
        autorp   (5) indicates entry was learnt from 
        Auto-Rendezvous-Point protocol, a Cisco proprietary protocol
        to dynamically distribute Group to RP mappings within an 
        autonomous system."
		characteristics "-N cpimExtRPSetType -O 1.3.6.1.4.1.9.10.120.1.1.7.1.5 -T INTEGER -A NA -X equal -F 0"

		notaccessible enum cpimExtRPSetProtocolE	cpimExtRPSetProtocol
		description 	"The multicast routing protocol"
		characteristics "-N cpimExtRPSetProtocol -O 1.3.6.1.4.1.9.10.120.1.1.7.1.6 -T INTEGER -A NA -X equal -F 0"

		notaccessible octet[20]	cpimExtRPSetAddress
		description 	"The Inet Address of the RP.
        
        Type of InetAddress presented in this object
        is same as that of cpimExtRPSetAddrType in
        INDEX clause"
		characteristics "-N cpimExtRPSetAddress -O 1.3.6.1.4.1.9.10.120.1.1.7.1.7 -T STRING -A NA -X equal -F 0"

		readonly gauge	cpimExtRPSetHoldTime
		description 	"The holdtime of the RP entry.  In case of 'bsr'
        cpimExtRPSetType, if the local router is not the BSR, this
        value is 0."
		characteristics "-N cpimExtRPSetHoldTime -O 1.3.6.1.4.1.9.10.120.1.1.7.1.8 -T GAUGE -A RO -X equal -F 0"

		readonly gauge	cpimExtRPSetExpiryTime
		description 	"The minimum time remaining before the RP denoted by this
        entry will be declared down.  In case of 'bsr' 
        cpimExtRPSetType, if the local router is not the BSR, 
        this value is 0."
		characteristics "-N cpimExtRPSetExpiryTime -O 1.3.6.1.4.1.9.10.120.1.1.7.1.9 -T GAUGE -A RO -X equal -F 0"

		readonly int	cpimExtRPSetPriority
		description 	"The RP-priority in the C-RP Advertisements or the
        RP-Set messages from the BSR (with value 0
        denoting the highest priority, default 192).
        Note that this field is valid only when cPimRPSetType
        is 'bsr', and for other types, this value will be -1."
		characteristics "-N cpimExtRPSetPriority -O 1.3.6.1.4.1.9.10.120.1.1.7.1.10 -T INTEGER -A RO -X equal -F 0"

		readonly enum cpimExtRPSetRPActiveE	cpimExtRPSetRPActive
		description 	"Evaluates to 'true' if the RP in this entry is the
        currently active RP for the group-range."
		characteristics "-N cpimExtRPSetRPActive -O 1.3.6.1.4.1.9.10.120.1.1.7.1.11 -T INTEGER -A RO -X equal -F 0"

	)

	table	cpimExtCRPTable
	description "The (conceptual) table listing the IP multicast groups
        for which the local router is to advertise itself as a
        Candidate-RP when the value of cPimComponentCRPHoldTime is
        non-zero.  If this table is empty, then the local router
        will advertise itself as a Candidate-RP for all groups
        (providing the value of cPimComponentCRPHoldTime is non-
        zero)."
