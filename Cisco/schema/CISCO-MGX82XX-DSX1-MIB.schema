		characteristics "-N lineBERTPattern -O 1.3.6.1.4.1.351.110.4.3.1.1.1.1.13 -T INTEGER -A RW -X equal -F 0"

		readwrite enum lineBERTResultE	lineBERTResult
		description 	"This shows the result of the BERT."
		characteristics "-N lineBERTResult -O 1.3.6.1.4.1.351.110.4.3.1.1.1.1.14 -T INTEGER -A RW -X equal -F 0"

		readwrite enum bERTResultClrButtonE	bERTResultClrButton
		description 	"This clears the result of the BERT."
		characteristics "-N bERTResultClrButton -O 1.3.6.1.4.1.351.110.4.3.1.1.1.1.15 -T INTEGER -A RW -X equal -F 0"

	)

