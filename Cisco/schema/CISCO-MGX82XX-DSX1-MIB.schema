		description 	"Enable detection of line Loopback Codes"
		characteristics "-N lineLoopbackCodeDetection -O 1.3.6.1.4.1.351.110.4.3.1.1.1.1.11 -T INTEGER -A RW -X equal -F 0"

		readwrite enum lineBERTEnableE	lineBERTEnable
		description 	"This variable enables Bit Error Rate Testing(BERT)."
		characteristics "-N lineBERTEnable -O 1.3.6.1.4.1.351.110.4.3.1.1.1.1.12 -T INTEGER -A RW -X equal -F 0"

		readwrite enum lineBERTPatternE	lineBERTPattern
