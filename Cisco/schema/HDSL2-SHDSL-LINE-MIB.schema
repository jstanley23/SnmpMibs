	table	hdsl2ShdslSpanConfProfileTable
	description "This table supports definitions of span configuration
         profiles for SHDSL lines.  HDSL2 does not support these
         configuration options.  This table MUST be maintained
         in a persistent manner."
	characteristics "-K ???"
	(
		notaccessible octet[32]	hdsl2ShdslSpanConfProfileName
		description 	"This object is the unique index associated with this profile.
         Entries in this table are referenced via the object
         hdsl2ShdslSpanConfProfile in Hdsl2ShdslSpanConfEntry."
		characteristics "-N hdsl2ShdslSpanConfProfileName -O 1.3.6.1.2.1.10.48.1.10.1.1 -T STRING -A NA -X equal -F 0"

		readwrite enum hdsl2ShdslSpanConfWireInterfaceE	hdsl2ShdslSpanConfWireInterface
		description 	"This object configures the two-wire or optional four-wire,
         six-wire, or eight-wire operation for SHDSL lines."
		characteristics "-N hdsl2ShdslSpanConfWireInterface -O 1.3.6.1.2.1.10.48.1.10.1.2 -T INTEGER -A RW -X equal -F 0"

		readwrite gauge	hdsl2ShdslSpanConfMinLineRate
		description 	"This object configures the minimum transmission rate for
         the associated SHDSL Line in bits-per-second (bps) and includes
         both payload (user data) and any applicable framing overhead.
         If the minimum line rate equals the maximum line rate
         (hdsl2ShdslSpanMaxLineRate), the line rate is considered
         'fixed'.  If the minimum line rate is less than the
         maximum line rate, the line rate is considered
         'rate-adaptive'."
		characteristics "-N hdsl2ShdslSpanConfMinLineRate -O 1.3.6.1.2.1.10.48.1.10.1.3 -T GAUGE -A RW -X equal -F 0"

		readwrite gauge	hdsl2ShdslSpanConfMaxLineRate
		description 	"This object configures the maximum transmission rate for
         the associated SHDSL Line in bits-per-second (bps) and includes
         both payload (user data) and any applicable framing overhead.
         If the minimum line rate equals the maximum line rate
         (hdsl2ShdslSpanMaxLineRate), the line rate is considered
         'fixed'.  If the minimum line rate is less than the
         maximum line rate, the line rate is considered
         'rate-adaptive'."
		characteristics "-N hdsl2ShdslSpanConfMaxLineRate -O 1.3.6.1.2.1.10.48.1.10.1.4 -T GAUGE -A RW -X equal -F 0"

		readwrite enum hdsl2ShdslSpanConfPSDE	hdsl2ShdslSpanConfPSD
		description 	"This object configures use of symmetric/asymmetric PSD (Power
         Spectral Density) Mask for the associated SHDSL Line.  Support
         for symmetric PSD is mandatory for all supported data rates.
         Support for asymmetric PSD is optional."
		characteristics "-N hdsl2ShdslSpanConfPSD -O 1.3.6.1.2.1.10.48.1.10.1.5 -T INTEGER -A RW -X equal -F 0"

		readwrite octet[2]	hdsl2ShdslSpanConfTransmissionMode
		description 	"This object specifies the regional setting for the SHDSL
         line."
		characteristics "-N hdsl2ShdslSpanConfTransmissionMode -O 1.3.6.1.2.1.10.48.1.10.1.6 -T STRING -A RW -X equal -F 0"

		readwrite enum hdsl2ShdslSpanConfRemoteEnabledE	hdsl2ShdslSpanConfRemoteEnabled
		description 	"This object enables/disables support for remote management
         of the units in an SHDSL line from the STU-R via the EOC."
		characteristics "-N hdsl2ShdslSpanConfRemoteEnabled -O 1.3.6.1.2.1.10.48.1.10.1.7 -T INTEGER -A RW -X equal -F 0"

		readwrite enum hdsl2ShdslSpanConfPowerFeedingE	hdsl2ShdslSpanConfPowerFeeding
		description 	"This object enables/disables support for optional power
         feeding in an SHDSL line."
		characteristics "-N hdsl2ShdslSpanConfPowerFeeding -O 1.3.6.1.2.1.10.48.1.10.1.8 -T INTEGER -A RW -X equal -F 0"

		readwrite int	hdsl2ShdslSpanConfCurrCondTargetMarginDown
		description 	"This object specifies the downstream current condition target
         SNR margin for an SHDSL line.  The SNR margin is the difference
         between the desired SNR and the actual SNR.  Target SNR margin
         is the desired SNR margin for a unit."
		characteristics "-N hdsl2ShdslSpanConfCurrCondTargetMarginDown -O 1.3.6.1.2.1.10.48.1.10.1.9 -T INTEGER -A RW -X equal -F 0"

		readwrite int	hdsl2ShdslSpanConfWorstCaseTargetMarginDown
		description 	"This object specifies the downstream worst-case target SNR
         margin for an SHDSL line.  The SNR margin is the difference
         between the desired SNR and the actual SNR.  Target SNR
         margin is the desired SNR margin for a unit."
		characteristics "-N hdsl2ShdslSpanConfWorstCaseTargetMarginDown -O 1.3.6.1.2.1.10.48.1.10.1.10 -T INTEGER -A RW -X equal -F 0"

		readwrite int	hdsl2ShdslSpanConfCurrCondTargetMarginUp
		description 	"This object specifies the upstream current-condition target
         SNR margin for an SHDSL line.  The SNR margin is the difference
         between the desired SNR and the actual SNR.  Target SNR margin
         is the desired SNR margin for a unit."
		characteristics "-N hdsl2ShdslSpanConfCurrCondTargetMarginUp -O 1.3.6.1.2.1.10.48.1.10.1.11 -T INTEGER -A RW -X equal -F 0"

		readwrite int	hdsl2ShdslSpanConfWorstCaseTargetMarginUp
		description 	"This object specifies the upstream worst-case target SNR
         margin for an SHDSL line.  The SNR margin is the difference
         between the desired SNR and the actual SNR.  Target SNR margin
         is the desired SNR margin for a unit."
		characteristics "-N hdsl2ShdslSpanConfWorstCaseTargetMarginUp -O 1.3.6.1.2.1.10.48.1.10.1.12 -T INTEGER -A RW -X equal -F 0"

		readwrite octet[2]	hdsl2ShdslSpanConfUsedTargetMargins
		description 	"Indicates whether a target SNR margin is enabled or
         disabled.  This is a bit-map of possible settings.  The
         various bit positions are as follows:
        
         currCondDown   - current-condition downstream target SNR
                          margin enabled
        
         worstCaseDown  - worst-case downstream target SNR margin
                          enabled
        
         currCondUp     - current-condition upstream target SNR
                          margin enabled
        
         worstCaseUp    - worst-case upstream target SNR margin
                          enabled."
		characteristics "-N hdsl2ShdslSpanConfUsedTargetMargins -O 1.3.6.1.2.1.10.48.1.10.1.13 -T STRING -A RW -X equal -F 0"

		readwrite enum hdsl2ShdslSpanConfReferenceClockE	hdsl2ShdslSpanConfReferenceClock
		description 	"This object configures the clock reference for the STU-C
         in an SHDSL Line."
		characteristics "-N hdsl2ShdslSpanConfReferenceClock -O 1.3.6.1.2.1.10.48.1.10.1.14 -T INTEGER -A RW -X equal -F 0"

		readwrite enum hdsl2ShdslSpanConfLineProbeEnableE	hdsl2ShdslSpanConfLineProbeEnable
		description 	"This object enables/disables support for Line Probe of
         the units in an SHDSL line.  When Line Probe is enabled, the
         system performs Line Probing to find the best possible
         rate.  If Line Probe is disabled, the rate adaptation phase
         is skipped to shorten set up time."
		characteristics "-N hdsl2ShdslSpanConfLineProbeEnable -O 1.3.6.1.2.1.10.48.1.10.1.15 -T INTEGER -A RW -X equal -F 0"

		readwrite enum hdsl2ShdslSpanConfProfileRowStatusE	hdsl2ShdslSpanConfProfileRowStatus
		description 	"This object controls creation/deletion of the associated
         entry in this table per the semantics of RowStatus.  If an
         active entry is referenced in hdsl2ShdslSpanConfProfile, the
         entry MUST remain active until all references are removed."
		characteristics "-N hdsl2ShdslSpanConfProfileRowStatus -O 1.3.6.1.2.1.10.48.1.10.1.16 -T INTEGER -A RW -X equal -F 0"

	)

