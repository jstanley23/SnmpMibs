		characteristics "-N cippfIpFilterICMPCode -O 1.3.6.1.4.1.9.9.278.1.1.3.1.21 -T INTEGER -A RW -X equal -F 0"

		readwrite octet[64]	cippfIpFilterSrcIPGroupName
		description 	"This object identifies the filter group 
        containing information about source IP 
        addresses and masks that need  to be matched
        for this filter.  This value must match an entry
        in cfgFilterGroupTable whose cfgFilterGroupType
        value is set to 'network' and must match an entry 
        in cfgFilterNetworkGroupTable.
        This object cannot be set if any of the following 
        objects are set:
            cippfIpFilterSrcAddress 
            cippfIpFilterSrcMask.
        
        This object is not instantiated if not applicable."
		characteristics "-N cippfIpFilterSrcIPGroupName -O 1.3.6.1.4.1.9.9.278.1.1.3.1.22 -T STRING -A RW -X equal -F 0"

		readwrite octet[64]	cippfIpFilterDstIPGroupName
		description 	"This object identifies the filter group 
        containing information about destination IP
        addresses and masks that need  to be matched
        for this filter.  This value must match an entry
        in cfgFilterGroupTable whose cfgFilterGroupType
        value is set to 'network' and must match an entry 
        in cfgFilterNetworkGroupTable.
        This object cannot be set if any of the following 
        objects are set:
            cippfIpFilterDestAddress 
            cippfIpFilterDestMask.
        
        This object is not instantiated if not applicable."
		characteristics "-N cippfIpFilterDstIPGroupName -O 1.3.6.1.4.1.9.9.278.1.1.3.1.23 -T STRING -A RW -X equal -F 0"

		readwrite octet[64]	cippfIpFilterProtocolGroupName
		description 	"This object identifies the filter group 
        containing information about internet protocol
        numbers.  This value must match an entry
        in cfgFilterGroupTable whose cfgFilterGroupType
        value is set to 'service' and must match an entry 
        in cfgFilterServiceGroupTable.
        This object cannot be set if any of the following 
        objects are set:
            cippfIpFilterProtocol.
        
        This object is not instantiated if not applicable."
		characteristics "-N cippfIpFilterProtocolGroupName -O 1.3.6.1.4.1.9.9.278.1.1.3.1.24 -T STRING -A RW -X equal -F 0"

		readwrite octet[64]	cippfIpFilterSrcServiceGroupName
		description 	"This object identifies the filter group 
        containing information about port(TCP/UDP)
        numbers.  This value must match an entry
        in cfgFilterGroupTable whose cfgFilterGroupType
        value is set to 'service' and must match an entry 
        in cfgFilterServiceGroupTable.
        This object cannot be set if any of the following 
        objects are set:
            cippfIpFilterSrcPortLow
            cippfIpFilterSrcPortHigh
        
        This object is not instantiated if not applicable."
		characteristics "-N cippfIpFilterSrcServiceGroupName -O 1.3.6.1.4.1.9.9.278.1.1.3.1.25 -T STRING -A RW -X equal -F 0"

		readwrite octet[64]	cippfIpFilterDstServiceGroupName
		description 	"This object identifies the filter group 
        containing information about port(TCP/UDP)
        numbers.  This value must match an entry
        in cfgFilterGroupTable whose cfgFilterGroupType
        value is set to 'service' and must match an entry 
        in cfgFilterServiceGroupTable.
        This object cannot be set if any of the following 
        objects are set:
            cippfIpFilterDestPortLow
            cippfIpFilterDestPortHigh
        
        This object is not instantiated if not applicable."
		characteristics "-N cippfIpFilterDstServiceGroupName -O 1.3.6.1.4.1.9.9.278.1.1.3.1.26 -T STRING -A RW -X equal -F 0"

		readwrite octet[64]	cippfIpFilterICMPGroupName
		description 	"This object identifies the filter group containing
        information about ICMP message.  This value must 
        match an entry in cfgFilterGroupTable whose 
        cfgFilterGroupType value is set to 'icmp' and must 
        match an entry in cfgFilterICMPGroupTable.
        This object cannot be set if any of the following 
        objects are set:
            cippfIpFilterICMPType 
            cippfIpFilterICMPCode.
        
        This object is not instantiated if not applicable."
		characteristics "-N cippfIpFilterICMPGroupName -O 1.3.6.1.4.1.9.9.278.1.1.3.1.27 -T STRING -A RW -X equal -F 0"

	)

	table	cippfIpFilterExtTable
	description "This table is an extension to cippfIpFilterTable.
        This table is used for configuring the objects
        that are used for reporting the information about
        filters and reporting the logs.  These objects
        do not change the match criteria for the filter
        but assist in troubleshooting the matched criteria."
	characteristics "-K ???"
	(
		readwrite octet[255]	cippfIpFilterExtDescription
		description 	"This object is used for configuring
        description of the filter."
		characteristics "-N cippfIpFilterExtDescription -O 1.3.6.1.4.1.9.9.278.1.1.4.1.1 -T STRING -A RW -X equal -F 0"

		readwrite enum cippfIpFilterExtLogLevelE	cippfIpFilterExtLogLevel
		description 	"This object is used for specifying the log level
        (severity) used in syslog for this filter."
		characteristics "-N cippfIpFilterExtLogLevel -O 1.3.6.1.4.1.9.9.278.1.1.4.1.2 -T INTEGER -A RW -X equal -F 0"

		readwrite gauge	cippfIpFilterExtLogInterval
		description 	"This object is used for configuring the time 
        interval at which the syslog message for this 
        filter to be generated."
		characteristics "-N cippfIpFilterExtLogInterval -O 1.3.6.1.4.1.9.9.278.1.1.4.1.3 -T GAUGE -A RW -X equal -F 0"

	)

	table	cippfIpFilterStatsTable
	description "This table defines a set of statistics related to 
        packet filter.  The statistics related to matched 
        filters are available here."
	characteristics "-K ???"
	(
		readonly counter	cippfIpFilterHits
		description 	"This object specifies the number of packets that
        are matched the packet filter configuration in
        cippfIpFilterTable."
		characteristics "-N cippfIpFilterHits -O 1.3.6.1.4.1.9.9.278.1.2.1.1.1 -T COUNTER -A RO -X equal -F 0"

	)

