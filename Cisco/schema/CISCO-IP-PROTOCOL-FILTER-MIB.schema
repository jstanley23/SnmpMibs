		characteristics "-N cippfIpFilterSrcMask -O 1.3.6.1.4.1.9.9.278.1.1.3.1.6 -T STRING -A RW -X equal -F 0"

		readwrite octet[255]	cippfIpFilterDestAddress
		description 	"The destination IP address to be matched for this 
