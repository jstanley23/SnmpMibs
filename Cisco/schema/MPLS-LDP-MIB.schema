		readonly gauge	mplsLdpSesMaxPduLen
		description 	"The value of maximum allowable length for LDP PDUs for
        this session.  This value may have been negotiated during
        the Session Initialization."
		characteristics "-N mplsLdpSesMaxPduLen -O 1.3.6.1.4.1.9.10.65.1.3.4.1.4 -T GAUGE -A RO -X equal -F 0"

		readonly timeticks	mplsLdpSesDiscontinuityTime
		description 	"The value of sysUpTime on the most recent occasion at
        which any one or more of this session's counters
        suffered a discontinuity.  The relevant counters are
        the specific instances associated with this session
        of any Counter32 or Counter64 object contained in the
        mplsLdpSessionStatsTable.
        
        The initial value of this object is the value of
        sysUpTime when the entry was created in this table.
        
        Also, an NMS can distinguish when a session
        between a given Entity and Peer goes away and then is
        're-established'.  This value would change and
        thus indicate to the NMS that this is a
        different session."
		characteristics "-N mplsLdpSesDiscontinuityTime -O 1.3.6.1.4.1.9.10.65.1.3.4.1.5 -T TIMETICKS -A RO -X equal -F 0"

	)

	table	mplsLdpAtmSesTable
	description "A table which relates Sessions in the
        'mplsLdpSessionTable' and their label
        range intersections.  There could be one
        
        
        or more label range intersections between an
        LDP Entity and LDP Peer using ATM as the underlying
        media. Each row represents a single label range
        intersection.
        
        NOTE:  this table cannot use the 'AUGMENTS'
        clause because there is not necessarily a one-to-one
        mapping between this table and the mplsLdpSessionTable."
	characteristics "-K TWO"
	(
		notaccessible int	mplsLdpSesAtmLRLowerBoundVpi
		description 	"The minimum VPI number for this range."
		characteristics "-N mplsLdpSesAtmLRLowerBoundVpi -O 1.3.6.1.4.1.9.10.65.1.3.5.1.1 -T INTEGER -A NA -X equal -F 0"

		notaccessible int	mplsLdpSesAtmLRLowerBoundVci
		description 	"The minimum VCI number for this range."
		characteristics "-N mplsLdpSesAtmLRLowerBoundVci -O 1.3.6.1.4.1.9.10.65.1.3.5.1.2 -T INTEGER -A NA -X equal -F 0"

		readonly int	mplsLdpSesAtmLRUpperBoundVpi
		description 	"The maximum VPI number for this range."
		characteristics "-N mplsLdpSesAtmLRUpperBoundVpi -O 1.3.6.1.4.1.9.10.65.1.3.5.1.3 -T INTEGER -A RO -X equal -F 0"

		readonly int	mplsLdpSesAtmLRUpperBoundVci
		description 	"The maximum VCI number for this range."
		characteristics "-N mplsLdpSesAtmLRUpperBoundVci -O 1.3.6.1.4.1.9.10.65.1.3.5.1.4 -T INTEGER -A RO -X equal -F 0"

	)

	table	mplsLdpFrameRelaySesTable
	description "A table of Frame Relay label range intersections
        between the LDP Entities and LDP Peers.
        Each row represents a single label range intersection.
        
        NOTE:  this table cannot use the 'AUGMENTS'
        clause because there is not necessarily a one-to-one
        mapping between this table and the mplsLdpSessionTable."
	characteristics "-K ONE"
	(
		notaccessible int	mplsLdpFrSesMinDlci
		description 	"The lower bound of DLCIs which are supported."
		characteristics "-N mplsLdpFrSesMinDlci -O 1.3.6.1.4.1.9.10.65.1.3.6.1.1 -T INTEGER -A NA -X equal -F 0"

		readonly int	mplsLdpFrSesMaxDlci
		description 	"The upper bound of DLCIs which are supported."
		characteristics "-N mplsLdpFrSesMaxDlci -O 1.3.6.1.4.1.9.10.65.1.3.6.1.2 -T INTEGER -A RO -X equal -F 0"

		readonly enum mplsLdpFrSesLenE	mplsLdpFrSesLen
		description 	"This object specifies the DLCI bits."
		characteristics "-N mplsLdpFrSesLen -O 1.3.6.1.4.1.9.10.65.1.3.6.1.3 -T INTEGER -A RO -X equal -F 0"

	)

	table	mplsLdpSesStatsTable
	description "A table of statistics for Sessions between
        
        
        LDP Entities and LDP Peers."
	characteristics "-K ???"
	(
		readonly counter	mplsLdpSesStatsUnkMesTypeErrors
		description 	"This object counts the number of Unknown Message Type
        Errors detected during this session.
        
        Discontinuities in the value of this counter can occur
        at re-initialization of the management system, and at
        other times as indicated by the value of
        mplsLdpSesDiscontinuityTime."
		characteristics "-N mplsLdpSesStatsUnkMesTypeErrors -O 1.3.6.1.4.1.9.10.65.1.3.7.1.1 -T COUNTER -A RO -X equal -F 0"

		readonly counter	mplsLdpSesStatsUnkTlvErrors
		description 	"This object counts the number of Unknown TLV Errors
        detected during this session.
        
        Discontinuities in the value of this counter can occur
        at re-initialization of the management system, and at
        other times as indicated by the value of
        mplsLdpSessionDiscontinuityTime."
		characteristics "-N mplsLdpSesStatsUnkTlvErrors -O 1.3.6.1.4.1.9.10.65.1.3.7.1.2 -T COUNTER -A RO -X equal -F 0"

	)

	table	mplsLdpSesInLabelMapTable
	description "A table of Session's Ingress Labels which
        are Mapped to the LSR MIB's mplsInSegmentTable.
        Each row represents a single Ingress Label."
	characteristics "-K TWO"
	(
		notaccessible int	mplsLdpSesInLabelIfIndex
		description 	"The ifIndex of the 'mplsLdpSesInLabel' which should
        have the same value as the 'mplsInSegmentIfIndex' in
        the LSR MIB."
		characteristics "-N mplsLdpSesInLabelIfIndex -O 1.3.6.1.4.1.9.10.65.1.3.9.1.1 -T INTEGER -A NA -X equal -F 0"

		notaccessible gauge	mplsLdpSesInLabel
		description 	"The incoming label of this LSP.  This has the same value
        as the 'mplsInSegmentLabel' in the LSR MIB."
		characteristics "-N mplsLdpSesInLabel -O 1.3.6.1.4.1.9.10.65.1.3.9.1.2 -T GAUGE -A NA -X equal -F 0"

		readonly enum mplsLdpSesInLabelTypeE	mplsLdpSesInLabelType
		description 	"The Layer 2 Label Type for 'mplsLdpInLabel'."
		characteristics "-N mplsLdpSesInLabelType -O 1.3.6.1.4.1.9.10.65.1.3.9.1.3 -T INTEGER -A RO -X equal -F 0"

		readonly enum mplsLdpSesInLabelConnTypeE	mplsLdpSesInLabelConnType
		description 	"The type of LSP connection.
        The possible values are:
        
        unknown(1) --    this may be the value if the LSP
                         is in a state of flux.  It is
                         considered to be a temporary
                         situation.
        xconnect(2) --   if the mapping between the
                         session and the insegment
                         is associated with an LSP which
                         is a true cross-connection.
        terminates(3) -- if the mapping between the
                         session and the insegment
                         is associated with an LSP which
                         terminates on this LSR and is
                         not a cross-connection."
		characteristics "-N mplsLdpSesInLabelConnType -O 1.3.6.1.4.1.9.10.65.1.3.9.1.4 -T INTEGER -A RO -X equal -F 0"

	)

	table	mplsLdpSesOutLabelMapTable
	description "A table of Session's Egress Labels which
        are Mapped to the LSR MIB.
        Each row represents a single Egress Label."
	characteristics "-K TWO"
	(
		notaccessible int	mplsLdpSesOutLabelIfIndex
		description 	"The ifIndex of the 'mplsLdpSesOutLabel'."
		characteristics "-N mplsLdpSesOutLabelIfIndex -O 1.3.6.1.4.1.9.10.65.1.3.10.1.1 -T INTEGER -A NA -X equal -F 0"

		notaccessible gauge	mplsLdpSesOutLabel
		description 	"The outgoing label of this LSP."
		characteristics "-N mplsLdpSesOutLabel -O 1.3.6.1.4.1.9.10.65.1.3.10.1.2 -T GAUGE -A NA -X equal -F 0"

		readonly enum mplsLdpSesOutLabelTypeE	mplsLdpSesOutLabelType
		description 	"The Layer 2 Label Type for 'mplsLdpOutLabel'."
		characteristics "-N mplsLdpSesOutLabelType -O 1.3.6.1.4.1.9.10.65.1.3.10.1.3 -T INTEGER -A RO -X equal -F 0"

		readonly enum mplsLdpSesOutLabelConnTypeE	mplsLdpSesOutLabelConnType
		description 	"The type of LSP connection.
        The possible values are:
        
        unknown(1) --  this may be the value if the LSP
                       is in a state of flux.  It is
                       considered to be a temporary
                       situation.
        xconnect(2) -- if the mapping between the
                       session and the outsegment
                       is associated with an LSP which
                       is a true cross-connection.
        starts(3) -- if the mapping between the
                      session and the insegment
                      is associated with an LSP which
                      starts on this LSR and is
                      considered an ingress to the LSP."
		characteristics "-N mplsLdpSesOutLabelConnType -O 1.3.6.1.4.1.9.10.65.1.3.10.1.4 -T INTEGER -A RO -X equal -F 0"

		readonly int	mplsLdpSesOutSegmentIndex
		description 	"This value should contain the same value as
        the 'mplsOutSegmentIndex' in the LSR MIB.
        
        NOTE: this value will never be zero, because
        this table only maps from Sessions to true
        outsegments."
		characteristics "-N mplsLdpSesOutSegmentIndex -O 1.3.6.1.4.1.9.10.65.1.3.10.1.5 -T INTEGER -A RO -X equal -F 0"

	)

	table	mplsLdpSesXCMapTable
	description "A table of Session's Labels which
        are Mapped to the LSR MIB 's XConnect table.
        Each row represents a single cross connect."
	characteristics "-K ???"
	(
		readonly int	mplsLdpSesXCIndex
		description 	"This value should contain the same value as
        the 'mplsXCIndex' in the LSR MIB.
        
        NOTE: this value will never be zero, because
        this table only maps from Sessions to true
        cross connects."
		characteristics "-N mplsLdpSesXCIndex -O 1.3.6.1.4.1.9.10.65.1.3.11.1.1 -T INTEGER -A RO -X equal -F 0"

	)

	table	mplsXCsFecsTable
	description "A table which shows the relationship between
        cross-connects and FECs.  Each row represents
        a single cross connect to FEC association.
        This table is read-only."
	characteristics "-K ???"
	(
		readonly enum mplsXCFecOperStatusE	mplsXCFecOperStatus
		description 	"An indication of the operational status of
        the FEC associated with this cross connect.
        
        unknown(1) - this is a temporary state which
                     may indicate the LSP-FEC association
                     is in a state of transition.
        
        inUse(2) - the FEC associated with the XC is
                   currently being applied.
        
        
        notInUse(3) - the FEC associated with the XC is
                      not being applied.  Eventually, this
                      entry may be aged out."
		characteristics "-N mplsXCFecOperStatus -O 1.3.6.1.4.1.9.10.65.1.3.13.1.1 -T INTEGER -A RO -X equal -F 0"

		readonly timeticks	mplsXCFecOperStatusLastChange
		description 	"This value of sysUpTime when the
        mplsXCFecOperStatus last changed state."
		characteristics "-N mplsXCFecOperStatusLastChange -O 1.3.6.1.4.1.9.10.65.1.3.13.1.2 -T TIMETICKS -A RO -X equal -F 0"

	)

	table	mplsLdpSesPeerAddrTable
	description "This table 'extends' the mplsLdpSessionTable.
        This table is used to store Label Address Information
        from Label Address Messages received by this LSR from
        Peers.  This table is read-only and should be updated
        when Label Withdraw Address Messages are received, i.e.
        Rows should be deleted as apropriate.
        
        NOTE:  since more than one address may be contained
        in a Label Address Message, this table 'extends',
        rather than 'AUGMENTS' the mplsLdpSessionTable's
        information."
	characteristics "-K ONE"
	(
		notaccessible gauge	mplsLdpSesPeerAddrIndex
		description 	"An index which uniquely identifies this entry within
        a given session."
		characteristics "-N mplsLdpSesPeerAddrIndex -O 1.3.6.1.4.1.9.10.65.1.3.12.1.1 -T GAUGE -A NA -X equal -F 0"

		readonly enum mplsLdpSesPeerNextHopAddrTypeE	mplsLdpSesPeerNextHopAddrType
		description 	"The internetwork layer address type of this Next Hop
        Address as specified in the Label Address Message
        associated with this Session. The value of this
        object indicates how to interpret the value of
        mplsLdpSessionPeerNextHopAddress."
		characteristics "-N mplsLdpSesPeerNextHopAddrType -O 1.3.6.1.4.1.9.10.65.1.3.12.1.2 -T INTEGER -A RO -X equal -F 0"

		readonly octet[64]	mplsLdpSesPeerNextHopAddr
		description 	"The value of the next hop address."
		characteristics "-N mplsLdpSesPeerNextHopAddr -O 1.3.6.1.4.1.9.10.65.1.3.12.1.3 -T STRING -A RO -X equal -F 0"

	)

	table	mplsLdpHelloAdjacencyTable
	description "A table of Hello Adjacencies for Sessions."
	characteristics "-K ONE"
	(
		notaccessible gauge	mplsLdpHelloAdjIndex
		description 	"An identifier for this specific adjacency."
		characteristics "-N mplsLdpHelloAdjIndex -O 1.3.6.1.4.1.9.10.65.1.3.2.1.1.1 -T GAUGE -A NA -X equal -F 0"

		readonly int	mplsLdpHelloAdjHoldTimeRem
		description 	"The time remaining for this Hello Adjacency.
        This interval will change when the 'next'
        Hello message which corresponds to this
        Hello Adjacency is received."
		characteristics "-N mplsLdpHelloAdjHoldTimeRem -O 1.3.6.1.4.1.9.10.65.1.3.2.1.1.2 -T INTEGER -A RO -X equal -F 0"

		readonly enum mplsLdpHelloAdjTypeE	mplsLdpHelloAdjType
		description 	"This adjacency is the result of a 'link'
        hello if the value of this object is link(1).
        Otherwise, it is a result of a 'targeted'
        hello, targeted(2)."
		characteristics "-N mplsLdpHelloAdjType -O 1.3.6.1.4.1.9.10.65.1.3.2.1.1.3 -T INTEGER -A RO -X equal -F 0"

	)

	group	mplsFecObjects
	description "(null)"
	characteristics "-K NONE"
	(
		readonly gauge	mplsFecIndexNext
		description 	"This object contains an appropriate value to
        be used for mplsFecIndex when creating
        entries in the mplsFecTable. The value
        0 indicates that no unassigned entries are
        available. To obtain the mplsFecIndex
        value for a new entry, the manager issues a
        management protocol retrieval operation to obtain
        the current value of this object.  After each
        retrieval, the agent should modify the value to
        the next unassigned index."
		characteristics "-N mplsFecIndexNext -O 1.3.6.1.4.1.9.10.65.1.3.8.1 -T GAUGE -A RO -X equal -F 0"

	)

	table	mplsFecTable
	description "This table represents the FEC
        (Forwarding Equivalence Class)
        Information associated with an LSP."
	characteristics "-K ONE"
	(
		notaccessible gauge	mplsFecIndex
		description 	"The index which uniquely identifies this entry."
		characteristics "-N mplsFecIndex -O 1.3.6.1.4.1.9.10.65.1.3.8.2.1.1 -T GAUGE -A NA -X equal -F 0"

		readwrite enum mplsFecTypeE	mplsFecType
		description 	"The type of the FEC.  If the value of this object
        is 'prefix(1)' then the FEC type described by this
        row is for address prefixes.
        
        If the value of this object is 'hostAddress(2)' then
        the FEC type described by this row is a host address."
		characteristics "-N mplsFecType -O 1.3.6.1.4.1.9.10.65.1.3.8.2.1.2 -T INTEGER -A RW -X equal -F 0"

		readwrite int	mplsFecAddrLength
		description 	"If the value of 'mplsFecType' is 'prefix(1)'
        then the value of this object is the length in
        bits of the address prefix represented by
        'mplsFecAddr', or if the length is zero then
        this is a special value which indicates that the
        prefix matches all addresses.  In this case the
        prefix is also zero (i.e. 'mplsFecAddr' will
        have the value of zero.)"
		characteristics "-N mplsFecAddrLength -O 1.3.6.1.4.1.9.10.65.1.3.8.2.1.3 -T INTEGER -A RW -X equal -F 0"

		readwrite enum mplsFecAddrFamilyE	mplsFecAddrFamily
		description 	"The value of this object is from the Address Family
        Numbers."
		characteristics "-N mplsFecAddrFamily -O 1.3.6.1.4.1.9.10.65.1.3.8.2.1.4 -T INTEGER -A RW -X equal -F 0"

		readwrite octet[64]	mplsFecAddr
		description 	"If the value of 'mplsFecType' is 'prefix(1)'
        then the value of this object is the address prefix.
        If the value of the 'mplsFecAddrLength'
        is object is zero, then this object should also be
        zero.
        
        If the value of the 'mplsFecType' is 'host(2)'
        then this is the host address."
		characteristics "-N mplsFecAddr -O 1.3.6.1.4.1.9.10.65.1.3.8.2.1.5 -T STRING -A RW -X equal -F 0"

		readwrite enum mplsFecStorTypeE	mplsFecStorType
		description 	"The storage type for this entry."
		characteristics "-N mplsFecStorType -O 1.3.6.1.4.1.9.10.65.1.3.8.2.1.6 -T INTEGER -A RW -X equal -F 0"

		readwrite enum mplsFecRowStatusE	mplsFecRowStatus
		description 	"An object that allows entries in this table to
         be created and deleted using the
         RowStatus convention."
		characteristics "-N mplsFecRowStatus -O 1.3.6.1.4.1.9.10.65.1.3.8.2.1.7 -T INTEGER -A RW -X equal -F 0"

	)


