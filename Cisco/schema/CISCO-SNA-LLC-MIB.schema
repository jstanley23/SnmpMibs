		characteristics "-N llcSapAdminAckTimer -O 1.3.6.1.4.1.9.10.8.1.2.1.1.6 -T TIMETICKS -A RW -X equal -F 0"

		readwrite timeticks	llcSapAdminPbitTimer
		description 	"This object denotes the default value for the
