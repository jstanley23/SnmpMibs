		characteristics "-N csiDeviceName -O 1.3.6.1.4.1.9.9.725.1.3.1.1.5 -T STRING -A RO -X equal -F 0"

		readonly octet[255]	csiDeviceBackupConfigFileName
		description 	"This object indicates the name of the latest saved
        configuration file of the device.  The file name is
        consisted of the device host name, MAC address and
        file extension 'REV2', for example
        'SwitchA-0021.d7ae.fc00.REV2'.  The file is stored in
        the host indicated by the csiBackupHostUrl object.
        
        If the device configuration file is saved again, .REV2
        file is copied to .REV1, and .REV2 is over-written with
        the latest device configuration."
		characteristics "-N csiDeviceBackupConfigFileName -O 1.3.6.1.4.1.9.9.725.1.3.1.1.6 -T STRING -A RO -X equal -F 0"

		readonly octet[255]	csiDeviceImageVersion
		description 	"This object indicates the software release version that the
        device is running.  The zero-length string indicates
        that no Version field (TLV) was reported in the most
        recent CDP message."
		characteristics "-N csiDeviceImageVersion -O 1.3.6.1.4.1.9.9.725.1.3.1.1.7 -T STRING -A RO -X equal -F 0"

		readonly octet[255]	csiDevicePlatform
		description 	"The device's hardware platform as reported in the most
        recent CDP message.  The zero-length string indicates
        that no Platform field (TLV) was reported in the most
        recent CDP message."
		characteristics "-N csiDevicePlatform -O 1.3.6.1.4.1.9.9.725.1.3.1.1.8 -T STRING -A RO -X equal -F 0"

		readonly octet[255]	csiDeviceSerialNum
		description 	"A recorded serial number for this device that indexes some
        type device catalog or inventory, if applicable."
		characteristics "-N csiDeviceSerialNum -O 1.3.6.1.4.1.9.9.725.1.3.1.1.9 -T STRING -A RO -X equal -F 0"

		readonly octet[255]	csiDeviceStatus
		description 	"A text string describes the network device status.  The agent
        implementation may use this object to report upgrading
        progress."
		characteristics "-N csiDeviceStatus -O 1.3.6.1.4.1.9.9.725.1.3.1.1.10 -T STRING -A RO -X equal -F 0"

	)

	group	csiNotifObjects
