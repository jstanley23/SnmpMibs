		readwrite octet[45]	cdot1agMaName
		description 	"The Short Maintenance Association name. The type/format of
         this object is determined by the value of the
         cdot1agMaNameType object."
		characteristics "-N cdot1agMaName -O 1.3.6.1.4.1.9.9.604.1.6.1.1.4 -T STRING -A RW -X equal -F 0"

		readwrite enum cdot1agMaMhfCreationE	cdot1agMaMhfCreation
		description 	"Indicates if the Management entity can create MHFs (MIP Half
         Function) for this MA."
		characteristics "-N cdot1agMaMhfCreation -O 1.3.6.1.4.1.9.9.604.1.6.1.1.5 -T INTEGER -A RW -X equal -F 0"

		readwrite enum cdot1agMaIdPermissionE	cdot1agMaIdPermission
		description 	"Enumerated value indicating what, if anything, is to be
